--query
select be.id, be.name, bu.name business_unit, bu.id business_unit_id, be.num_seats, be.num_subtests, be.payment_method, be.shipment_account_id, be.shipment_locator_id, be.billing_account_id, be.billing_locator_id,  to_char(be.last_updated, 'MM-dd-YYYY hh24:mi:ss') last_updated  from business_entity be, business_unit bu where be.parent_id = bu.id and  be.last_updated  > to_date('#LAST_RUN#', 'MM-dd-YYYY hh24:mi:ss') and be.last_updated < to_date('#CURRENT_RUN#', 'MM-dd-YYYY hh24:mi:ss') and $CONDITIONS