--query
select bel.name ,bel.id || '_' || t.id id, bel.start_billing_date, bel.expiration_date, bel.creation_date, l.name license_type , t.name test, be.name business_entity , bu.name business_unit, to_char(bel.last_updated, 'MM-dd-YYYY hh24:mi:ss') last_updated from business_entity_license bel, business_entity_lic_test belt, business_unit bu, business_entity be, license_type l, test t where belt.entity_license_id = bel.id and bel.business_entity_id = be.id and be.parent_id = bu.id and bel.license_type_id = l.id and belt.test_id = t.id and bel.last_updated > to_date('#LAST_RUN#', 'MM-dd-YYYY hh24:mi:ss') and $CONDITIONS